<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="swami_pages_session">
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">sa</property>
        <property name="hibernate.connection.url">jdbc:mysql://127.0.0.1:3306/swami_pages</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <!-- <property name="hibernate.hbm2ddl.auto">create</property> -->
        
        <mapping class="com.entity.SwamiLanguage" />
        <mapping class="com.entity.SwamiUser" />  
        <mapping class="com.entity.SwamiUserLogin" />
         <mapping class="com.entity.Category" />
        <mapping class="com.entity.Book" />  
        <mapping class="com.entity.Cerification" />  
        <mapping class="com.entity.Degree" />
        <mapping class="com.entity.EducationDetails" />
        <mapping class="com.entity.JobDetail" />  
        <mapping class="com.entity.JobSkill" />
        <mapping class="com.entity.JobSpecificationDetails" />  
        <mapping class="com.entity.JobType" />
        <mapping class="com.entity.Mantra" />  
        <mapping class="com.entity.Menu" />
        <mapping class="com.entity.Photo" />  
        <mapping class="com.entity.Poem" />
        <mapping class="com.entity.Story" />  
        <mapping class="com.entity.Video" />  
    </session-factory>
</hibernate-configuration>
